{
	"name": "vitest-browser-qwik",
	"version": "0.0.2",
	"packageManager": "pnpm@10.11.0",
	"description": "A starter for creating a tsdown package.",
	"type": "module",
	"license": "MIT",
	"homepage": "https://github.com/kunai-consulting/vitest-browser-qwik#readme",
	"bugs": {
		"url": "https://github.com/kunai-consulting/vitest-browser-qwik/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kunai-consulting/vitest-browser-qwik.git"
	},
	"author": "Kunai Consulting <kunaico.com>",
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"check": "biome ci .",
		"check.format": "biome format .",
		"check.lint": "biome check .",
		"fix": "pnpm lint && pnpm format",
		"format": "biome format --write .",
		"lint": "biome check --write --unsafe .",
		"build": "tsdown",
		"dev": "tsdown --watch",
		"test": "vitest",
		"test:unit": "vitest --config vitest.unit.config.ts",
		"typecheck": "tsc --noEmit",
		"release": "bumpp && pnpm publish",
		"prepublishOnly": "pnpm run build"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.0",
		"@builder.io/qwik": "1.14.1",
		"@oxc-project/types": "^0.73.2",
		"@types/node": "^22.15.17",
		"@vitest/browser": "^3.2.4",
		"bumpp": "^10.1.0",
		"magic-string": "^0.30.17",
		"oxc-parser": "^0.73.2",
		"oxc-resolver": "^11.2.0",
		"oxc-walker": "^0.3.0",
		"@playwright/test": "see flake.nix",
		"tsdown": "^0.11.9",
		"tsx": "^4.19.4",
		"typescript": "^5.8.3",
		"vitest": "^3.1.3"
	},
	"pnpm": {
		"overrides": {
			"@playwright/test": "1.52.0",
			"playwright": "1.52.0"
		}
	}
}
