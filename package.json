{
	"name": "vitest-browser-qwik",
	"version": "0.0.0",
	"packageManager": "pnpm@10.11.0",
	"description": "A starter for creating a tsdown package.",
	"type": "module",
	"license": "MIT",
	"homepage": "https://github.com/kunai-consulting/vitest-browser-qwik#readme",
	"bugs": {
		"url": "https://github.com/kunai-consulting/vitest-browser-qwik/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kunai-consulting/vitest-browser-qwik.git"
	},
	"author": "Kunai Consulting <kunaico.com>",
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"lint": "eslint --cache .",
		"lint:fix": "pnpm run lint --fix",
		"build": "tsdown",
		"dev": "tsdown --watch",
		"test": "vitest",
		"typecheck": "tsc --noEmit",
		"format": "prettier --cache --write .",
		"release": "bumpp && pnpm publish",
		"prepublishOnly": "pnpm run build"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.0",
		"@builder.io/qwik": "1.13",
		"@types/node": "^22.15.17",
		"@vitest/browser": "^3.2.4",
		"bumpp": "^10.1.0",
		"playwright": "^1.53.1",
		"tsdown": "^0.11.9",
		"tsx": "^4.19.4",
		"typescript": "^5.8.3",
		"vitest": "^3.1.3"
	}
}
